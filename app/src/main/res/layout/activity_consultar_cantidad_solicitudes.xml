<?xml version="1.0" encoding="utf-8"?>
<LinearLayout tools:context=".webServices.ConsultarCantidadSolicitudes"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/LinearLayout1"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView  android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="Consultar Cantidad de Solicitudes de Procecos Academicos desde Servidor EISI" />
    <TextView  android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="Ingrese Carnet para consultar" />
    <EditText  android:layout_height="wrap_content"
        android:id="@+id/carnet"

        android:layout_width="match_parent"
        android:hint="Ingrese CARNET"
        android:maxLength="7"
         />

    <LinearLayout
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <Button android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/diferidos"
            android:text="Cantidad Diferidos"
            android:onClick="diferidos"
            tools:ignore="OnClick" />
        <Button android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/repetidos"
            android:text="Cantidad Repetdos"
            android:onClick="repetidos"
            tools:ignore="OnClick" />
        <Button android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/primeraRevision"
            android:text="Cantidad primera Revision"
            android:onClick="primeraRevision"
            tools:ignore="OnClick" />
        <Button android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/segundaRevision"
            android:text="Cantidad segunda revision"
            android:onClick="segundaRevision"
            tools:ignore="OnClick" />
        <Button android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/limpiar"
            android:text="limpiar Datos"
            android:onClick="limpiarDatos"
            tools:ignore="OnClick" />

    </LinearLayout>

    <ListView
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/listViewCantidad">
    </ListView>


</LinearLayout>
